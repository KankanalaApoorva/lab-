#include<stdio.h>
#include<conio.h>
#include<math.h>
void main()
{
int x1,x2,y1,y2;
float distance;
printf("Enter the x and y coordinates of first point\n");
scanf("%d%d",&x1,&y1);
printf("Enter the x and y coordinates of second point\n");
scanf("%d%d",&x2,&y2);
distance=sqrt((x2-x1)*(x2-x1)+(y2-y1)*(y2-y1));
printf("The distance between the points is %f",distance);
getch();
}
